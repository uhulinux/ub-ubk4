+void ym2413_set_update_handler(void *chip,OPLL_UPDATEHANDLER UpdateHandler,void *param)
+{
+	YM2413 *OPLL = (YM2413 *)chip;
+	OPLLSetUpdateHandler(OPLL, UpdateHandler, param);
