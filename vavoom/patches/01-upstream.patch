+void ym2413_write(void *chip, int a, int v)
+{
+	YM2413 *OPLL = (YM2413 *)chip;
+	OPLLWrite(OPLL, a, v);
+}
+
+unsigned char ym2413_read(void *chip, int a)
+{
+	YM2413 *OPLL = (YM2413 *)chip;
+	return OPLLRead(OPLL, a) & 0x03 ;
+}
