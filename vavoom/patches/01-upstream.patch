+					op[ 1 ] += f[ i ] * ip[ 1 ];
+					op += 2;
+				}
+
+				op0 += opstep;
+				l--;
+			}
+		}
+
+		op = op0;
+		const fptype* dc = SuffixDC;
+		l = SuffixDC.getCapacity();
+
+		if( ElCount == 1 )
+		{
+			while( l > 0 )
+			{
+				op[ 0 ] += ip[ 0 ] * dc[ 0 ];
+				dc++;
+				op++;
+				l--;
+			}
+		}
+		else
+		if( ElCount == 4 )
+		{
+			while( l > 0 )
+			{
+				op[ 0 ] += ip[ 0 ] * dc[ 0 ];
+				op[ 1 ] += ip[ 1 ] * dc[ 0 ];
+				op[ 2 ] += ip[ 2 ] * dc[ 0 ];
+				op[ 3 ] += ip[ 3 ] * dc[ 0 ];
+				dc++;
+				op += 4;
+				l--;
+			}
+		}
+		else
+		if( ElCount == 3 )
+		{
+			while( l > 0 )
+			{
+				op[ 0 ] += ip[ 0 ] * dc[ 0 ];
+				op[ 1 ] += ip[ 1 ] * dc[ 0 ];
+				op[ 2 ] += ip[ 2 ] * dc[ 0 ];
+				dc++;
+				op += 3;
+				l--;
+			}
+		}
+		else
+		if( ElCount == 2 )
+		{
+			while( l > 0 )
+			{
+				op[ 0 ] += ip[ 0 ] * dc[ 0 ];
+				op[ 1 ] += ip[ 1 ] * dc[ 0 ];
+				dc++;
+				op += 2;
+				l--;
+			}
+		}
+
+		ip = Src;
+		op = Dst - InPrefix * opstep;
+		dc = PrefixDC;
+		l = PrefixDC.getCapacity();
+
+		if( ElCount == 1 )
+		{
+			while( l > 0 )
+			{
+				op[ 0 ] += ip[ 0 ] * dc[ 0 ];
+				dc++;
+				op++;
+				l--;
+			}
+		}
+		else
+		if( ElCount == 4 )
+		{
+			while( l > 0 )
+			{
+				op[ 0 ] += ip[ 0 ] * dc[ 0 ];
+				op[ 1 ] += ip[ 1 ] * dc[ 0 ];
+				op[ 2 ] += ip[ 2 ] * dc[ 0 ];
+				op[ 3 ] += ip[ 3 ] * dc[ 0 ];
+				dc++;
+				op += 4;
+				l--;
+			}
+		}
+		else
+		if( ElCount == 3 )
+		{
+			while( l > 0 )
+			{
+				op[ 0 ] += ip[ 0 ] * dc[ 0 ];
+				op[ 1 ] += ip[ 1 ] * dc[ 0 ];
+				op[ 2 ] += ip[ 2 ] * dc[ 0 ];
+				dc++;
+				op += 3;
+				l--;
+			}
+		}
+		else
+		if( ElCount == 2 )
+		{
+			while( l > 0 )
+			{
+				op[ 0 ] += ip[ 0 ] * dc[ 0 ];
+				op[ 1 ] += ip[ 1 ] * dc[ 0 ];
+				dc++;
+				op += 2;
+				l--;
+			}
+		}
+	}
