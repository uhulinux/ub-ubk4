+	{
+		int temp;
+	case 0xBE: // CP (HL)
+		temp = READ_MEM( rp.hl );
+		goto cmp_comm;
+	
+	case 0xB8: // CP B
+	case 0xB9: // CP C
+	case 0xBA: // CP D
+	case 0xBB: // CP E
+	case 0xBC: // CP H
+	case 0xBD: // CP L
+	case 0xBF: // CP A
+		temp = R8( op & 7 );
+	cmp_comm:
+		ph = rg.a ^ temp; // N=1 H=*
+		cz = rg.a - temp; // C=* Z=*
+		goto loop;
+	}
+	
+	case 0xFE: // CP IMM
+		pc++;
+		ph = rg.a ^ data; // N=1 H=*
+		cz = rg.a - data; // C=* Z=*
+		goto loop;
