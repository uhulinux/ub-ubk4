--- orig/source/gme/src/divfix.h	1970-01-01 01:00:00.000000000 +0100
+++ mod/source/gme/src/divfix.h	2020-05-23 18:21:13.000000000 +0200
@@ -0,0 +1,18 @@
+
+static Uint32 DivFix(Uint32 p1, Uint32 p2, Uint32 fix)
+{
+	Uint32 ret;
+	ret = p1 / p2;
+	p1  = p1 % p2;/* p1 = p1 - p2 * ret; */
+	while (fix--)
+	{
+		p1 += p1;
+		ret += ret;
+		if (p1 >= p2)
+		{
+			p1 -= p2;
+			ret++;
+		}
+	}
+	return ret;
+}
