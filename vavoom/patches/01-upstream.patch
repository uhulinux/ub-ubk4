+void YM2612Mute(YM2612* F2612, int mask)
+{
+	int c;
+	for ( c = 0; c < 6; c++ )
+	{
+		F2612->OPN.pan_mutes [c] = -(mask >> c & 1);
+		OPNUpdatePan( &F2612->OPN, c );
+	}
+}
