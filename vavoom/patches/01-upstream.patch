+	~Sms_Fm_Apu();
+	BLARGG_DISABLE_NOTHROW
+	enum { osc_count = 1 };
+	void set_output( int i, Blip_Buffer* b, Blip_Buffer* = NULL, Blip_Buffer* = NULL ) { output_ = b; }
+
+private:
+	Blip_Buffer* output_;
+	blip_time_t next_time;
+	int last_amp;
+	int addr;
+	
+	int clock_;
+	int rate_;
+	blip_time_t period_;
+	
+	Blip_Synth_Norm synth;
+	Ym2413_Emu apu;
+	
+	void run_until( blip_time_t );
+};
+
+#endif
