+void Effects_Buffer::clear()
+{
+	echo_pos       = 0;
+	s.low_pass [0] = 0;
+	s.low_pass [1] = 0;
+	mixer.samples_read = 0;
+	
+	for ( int i = bufs_size; --i >= 0; )
+		bufs [i].clear();
