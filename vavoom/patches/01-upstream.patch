+inline static void LfoStep(OPL_LFO* lfop )
+{
+	lfop->cnt += lfop->sps;
+	lfop->adr += lfop->cnt >> LFO_SHIFT;
+	lfop->cnt &= (1 << LFO_SHIFT) - 1;
+	lfop->output = lfop->table [lfop->adr & lfop->adrmask];
+}
