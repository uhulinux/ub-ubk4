+    use crate::same_music::get_approximate_conversion;
+
+    #[test]
+    fn test_strings() {
+        let mut what = "roman ( ziemniak ) ".to_string();
+        get_approximate_conversion(&mut what);
+        assert_eq!(what, "roman");
+
+        let mut what = "  HH)    ".to_string();
+        get_approximate_conversion(&mut what);
+        assert_eq!(what, "HH");
+    }
+}
