+                writeln!(writer).unwrap();
+            }
+        } else {
+            write!(writer, "Not found any similar videos.").unwrap();
+        }
+
+        Common::print_time(start_time, SystemTime::now(), "save_results_to_file".to_string());
+        true
+    }
